<Window x:Class="StatePokemon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Evolve"  Click="EvolveMe"/>
        <TextBlock x:Name="tbState" Grid.Column="1" />
        <Button Content="Evolve Back" Grid.Column="2"  Click="DisEvolve"/>
        
    </Grid>
</Window>
